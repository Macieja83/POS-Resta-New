{
  "name": "@pos-system/backend",
  "version": "1.0.0",
  "description": "POS System Backend API",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "build": "tsc",
    "start": "tsx src/server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "vitest",
    "test:e2e": "vitest run tests/*.e2e.spec.ts",
    "test:unit": "vitest run tests/*.service.spec.ts",
    "db:check": "node check-db-connection.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "optimize": "node optimize-performance.js",
    "vercel-build": "npx prisma generate && npm run build"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "supertest": "^6.3.3",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
